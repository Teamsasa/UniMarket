{
  "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "strict": true,
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./src",
      "paths": {
          "@components/*": ["components/*"]
      }
  },
  "include": ["src/**/*"]
}
